# Tang Primer 25K ピン配置ファイル (.cst)
# GW5A-LV25MG121NC1/I0 用

# システムクロック (50MHz)
# 注意: E2は専用ピンのため、Configurationで "Use SSPI as regular IO" を設定してください
IO_LOC "clk" E2;
IO_PORT "clk" PULL_MODE=NONE IO_TYPE=LVCMOS33;

# リセットボタン (S1)
IO_LOC "reset_btn" K10;
IO_PORT "reset_btn" PULL_MODE=UP IO_TYPE=LVCMOS33;

# 動作確認用LED (Dock LED0)
IO_LOC "led" D10;
IO_PORT "led" IO_TYPE=LVCMOS33 PULL_MODE=UP;

# VGA 出力ピン (J5ヘッダー)
# HSYNC
IO_LOC "vga_hsync" E11;
IO_PORT "vga_hsync" DRIVE=8 IO_TYPE=LVCMOS33;

# VSYNC
IO_LOC "vga_vsync" D11;
IO_PORT "vga_vsync" DRIVE=8 IO_TYPE=LVCMOS33;

# RED信号
IO_LOC "vga_r" C11;
IO_PORT "vga_r" DRIVE=8 IO_TYPE=LVCMOS33;

# GREEN信号
IO_LOC "vga_g" B11;
IO_PORT "vga_g" DRIVE=8 IO_TYPE=LVCMOS33;

# BLUE信号
IO_LOC "vga_b" A11;
IO_PORT "vga_b" DRIVE=8 IO_TYPE=LVCMOS33;
